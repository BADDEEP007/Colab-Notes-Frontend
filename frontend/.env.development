# Development Environment Configuration
VITE_ENV=development

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_SOCKET_URL=http://localhost:3000
VITE_API_TIMEOUT=10000

# OAuth Configuration (use test credentials for development)
VITE_GOOGLE_CLIENT_ID=
VITE_MICROSOFT_CLIENT_ID=

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_WHITEBOARD=true
VITE_ENABLE_OAUTH=true

# Application Configuration
VITE_APP_NAME=Collab Notes (Dev)
VITE_AUTOSAVE_DELAY=500
VITE_SOCKET_RECONNECT_ATTEMPTS=5
VITE_SOCKET_RECONNECT_DELAY=3000

# Development Configuration
VITE_DEBUG=true
VITE_ENABLE_DEVTOOLS=true
