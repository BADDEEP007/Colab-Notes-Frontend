# Staging Environment Configuration
VITE_ENV=staging

# API Configuration
VITE_API_BASE_URL=https://api-staging.collabnotes.com
VITE_SOCKET_URL=wss://socket-staging.collabnotes.com
VITE_API_TIMEOUT=10000

# OAuth Configuration (use staging credentials)
VITE_GOOGLE_CLIENT_ID=your_staging_google_client_id
VITE_MICROSOFT_CLIENT_ID=your_staging_microsoft_client_id

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_WHITEBOARD=true
VITE_ENABLE_OAUTH=true

# Application Configuration
VITE_APP_NAME=Collab Notes (Staging)
VITE_AUTOSAVE_DELAY=500
VITE_SOCKET_RECONNECT_ATTEMPTS=5
VITE_SOCKET_RECONNECT_DELAY=3000

# Analytics & Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Development Configuration
VITE_DEBUG=false
VITE_ENABLE_DEVTOOLS=false
